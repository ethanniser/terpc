{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "globalDependencies": ["**/.env.*local"],
  "globalEnv": [],
  "globalPassThroughEnv": [
    "VERCEL_URL",
    "VERCEL_ENV",
    "NODE",
    "NODE_ENV",
    "CI"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "//#test": {
      "dependsOn": []
    },
    "test": {
      "cache": false,
      "dependsOn": []
    },
    "test:watch": {
      "cache": false
    }
  }
}
